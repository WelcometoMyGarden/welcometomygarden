{
  "name": "welcome-to-my-garden",
  "license": "AGPL-3.0-only",
  "description": "a platform allowing citizens to put their garden freely at disposal as a rudimentary campsite to travelers.",
  "version": "3.3.0",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite --host",
    "dev:staging": "vite --mode staging --host",
    "dev:push": "vite --mode devpush --host",
    "dev:prod": "vite --mode production --host",
    "build": "vite build",
    "build:demo": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "check": "svelte-kit sync && svelte-check --compiler-warnings 'security-anchor-rel-noreferrer:ignore' --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --compiler-warnings 'security-anchor-rel-noreferrer:ignore' --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "firebase:demo": "firebase --project demo-test emulators:start",
    "firebase:demo-seed": "firebase --project demo-test emulators:exec --ui api/seeders/simple.js",
    "firebase:debug": "firebase --project demo-test emulators:start --inspect-functions",
    "firebase:staging": "firebase --project staging emulators:start --only functions,tasks",
    "firebase:staging-all": "firebase --project staging emulators:start",
    "firebase:staging-push": "STAGING=true firebase --project staging emulators:exec --ui api/seeders/simple.js",
    "stripe:staging": "stripe listen --events customer.subscription.deleted,customer.subscription.updated,invoice.finalized,invoice.created,invoice.paid,payment_intent.processing --forward-to http://127.0.0.1:5001/wtmg-dev/europe-west1/handleStripeWebhookV2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@capacitor-community/safe-area": "^7.0.0-alpha.1",
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/device": "^7.0.2",
    "@capacitor/inappbrowser": "^2.3.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/local-notifications": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.2",
    "@capacitor/splash-screen": "^7.0.2",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@playwright/test": "^1.52.0",
    "@sentry/sveltekit": "^9.33.0",
    "@stripe/stripe-js": "^4.10.0",
    "@supabase/supabase-js": "^2.50.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "2.21.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tmcw/togeojson": "^5.8.1",
    "@turf/turf": "^7.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/nprogress": "^0.2.3",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@zerodevx/svelte-img": "^2.1.2",
    "capacitor-swipe-back-plugin": "https://storage.googleapis.com/wtmg-static/packages/capacitor-swipe-back-plugin-3.0.0.tgz",
    "dotenv": "^16.4.5",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "file-selector": "^1.1.0",
    "firebase": "^11.10.0",
    "gmail-getter": "^2.2.0",
    "i18n-iso-countries": "^7.14.0",
    "iso-639-1": "^3.1.5",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^1.15.3",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "smoothscroll-polyfill": "^0.4.4",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.5",
    "svelte-i18n": "^4.0.1",
    "svelte-preprocess": "^6.0.3",
    "svelte-simple-modal": "^1.6.2",
    "svelte-stripe": "^1.3.0",
    "svg-inline-loader": "^0.8.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "ua-parser-js": "2.0.0-beta.1",
    "vite": "^5.4.18",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^2.1.9"
  },
  "type": "module",
  "packageManager": "yarn@4.9.0"
}
