{
  "name": "welcome-to-my-garden",
  "description": "a platform allowing citizens to put their garden freely at disposal as a rudimentary campsite to travelers.",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env routify -b --dynamic-imports && rollup -c",
    "build:prod": "cross-env NODE_ENV=production yarn run build",
    "build:staging": "cross-env NODE_ENV=staging yarn run build",
    "dev": "cross-env BUNDLING=dynamic run-p routify nollup",
    "start": "sirv dist --single",
    "deploy": "yarn run build && firebase deploy",
    "routify": "routify",
    "nollup": "nollup -c"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-geocoder": "^4.5.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@sveltech/routify": "^1.9.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "firebase": "^7.14.6",
    "maplibre-gl": "^1.14.1-rc.2",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "nollup": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.0.0",
    "svelte-i18n": "^3.0.4",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-window": "^5.1.3"
  }
}
